{
  "name": "wanderplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "vite --host",
    "dev:network": "echo 'Starting dev server on network...' && vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:mobile": "vite preview --host",
    "test": "npm run test:build && npm run test:qa",
    "test:build": "tsc -b --noEmit",
    "test:qa": "echo 'Open qa-test-suite.html in browser to run QA tests'",
    "precommit": "npm run test:build && npm run lint",
    "predeploy": "npm run test && echo '⚠️  Run qa-test-suite.html and complete PRE_DEPLOYMENT_CHECKLIST.md before deploying!'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google-cloud/functions-framework": "^4.0.0",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.55.0",
    "@types/leaflet": "^1.9.20",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "googleapis": "^155.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.535.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.11.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.7.1",
    "react-simple-maps": "^3.0.0",
    "recharts": "^3.1.0",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/google.maps": "^3.58.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-simple-maps": "^3.0.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
