# Google Maps API (for location services and place search)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI API (for AI-powered itinerary generation)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Google OAuth (for Google sign-in)
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret_here

# Email Service (for password reset emails - optional in development)
VITE_EMAIL_SERVICE=gmail
VITE_EMAIL_USER=your_email@gmail.com
VITE_EMAIL_PASS=your_app_password_here
VITE_EMAIL_FROM_NAME=Wanderplan

# Optional configurations
VITE_GOOGLE_PLACES_LIBRARY=places
VITE_GOOGLE_PLACES_TYPES=establishment,geocode
VITE_OPENAI_MODEL=gpt-4o-mini
VITE_OPENAI_MAX_TOKENS=2000